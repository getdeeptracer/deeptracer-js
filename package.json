{
  "name": "deeptracer-js",
  "private": true,
  "version": "0.4.0",
  "description": "DeepTracer JavaScript SDK â€” multi-package observability toolkit",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build -w packages/core && npm run build -w packages/node -w packages/ai -w packages/browser && npm run build -w packages/react && npm run build -w packages/nextjs",
    "clean": "rm -rf packages/*/dist",
    "typecheck": "tsc --build packages/*/tsconfig.json",
    "lint": "eslint 'packages/*/src/**/*.{ts,tsx}'",
    "format": "prettier --write 'packages/*/src/**/*.{ts,tsx}' 'packages/*/tsup.config.ts' 'packages/*/tsconfig.json'",
    "format:check": "prettier --check 'packages/*/src/**/*.{ts,tsx}' 'packages/*/tsup.config.ts' 'packages/*/tsconfig.json'",
    "size": "size-limit"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@size-limit/esbuild": "^12.0.0",
    "@size-limit/file": "^12.0.0",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "size-limit": "^12.0.0",
    "tsup": "^8.0.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/getdeeptracer/deeptracer-js.git"
  },
  "license": "MIT"
}
