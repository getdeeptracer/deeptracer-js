{
  "name": "@deeptracer/nextjs",
  "version": "0.7.0",
  "description": "DeepTracer Next.js integration â€” automatic server-side error capture with one-file setup",
  "type": "module",
  "main": "dist/index.server.cjs",
  "module": "dist/index.server.js",
  "types": "dist/index.types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.types.d.ts",
      "edge-light": {
        "import": "./dist/index.edge.js",
        "require": "./dist/index.edge.cjs"
      },
      "worker": {
        "import": "./dist/index.edge.js",
        "require": "./dist/index.edge.cjs"
      },
      "node": {
        "import": "./dist/index.server.js",
        "require": "./dist/index.server.cjs"
      },
      "import": "./dist/index.server.js",
      "require": "./dist/index.server.cjs"
    },
    "./client": {
      "types": "./dist/index.client.d.ts",
      "import": "./dist/index.client.js",
      "require": "./dist/index.client.cjs"
    },
    "./universal": {
      "types": "./dist/index.universal.d.ts",
      "import": "./dist/index.universal.js",
      "require": "./dist/index.universal.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js",
      "require": "./dist/config.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "keywords": [
    "deeptracer",
    "nextjs",
    "observability",
    "error-tracking",
    "instrumentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/getdeeptracer/deeptracer-js.git",
    "directory": "packages/nextjs"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@deeptracer/core": "0.7.0",
    "@deeptracer/node": "0.7.0",
    "@deeptracer/react": "0.7.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "server-only": "^0.0.1"
  },
  "peerDependencies": {
    "@vercel/functions": ">=1",
    "next": ">=15",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "peerDependenciesMeta": {
    "@vercel/functions": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vercel/functions": "^3.4.3"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsup",
    "dev": "tsup --watch"
  }
}
