{
  "name": "@deeptracer/nextjs",
  "version": "0.4.0",
  "description": "DeepTracer Next.js integration â€” automatic server-side error capture with one-file setup",
  "type": "module",
  "main": "dist/index.server.cjs",
  "module": "dist/index.server.js",
  "types": "dist/index.types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.types.d.ts",
      "browser": {
        "import": "./dist/index.client.js",
        "require": "./dist/index.client.cjs"
      },
      "edge-light": {
        "import": "./dist/index.server.js",
        "require": "./dist/index.server.cjs"
      },
      "worker": {
        "import": "./dist/index.server.js",
        "require": "./dist/index.server.cjs"
      },
      "node": {
        "import": "./dist/index.server.js",
        "require": "./dist/index.server.cjs"
      },
      "import": "./dist/index.server.js",
      "require": "./dist/index.server.cjs"
    },
    "./client": {
      "types": "./dist/index.client.d.ts",
      "import": "./dist/index.client.js",
      "require": "./dist/index.client.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "sideEffects": false,
  "keywords": ["deeptracer", "nextjs", "observability", "error-tracking", "instrumentation"],
  "repository": {
    "type": "git",
    "url": "https://github.com/getdeeptracer/deeptracer-js.git",
    "directory": "packages/nextjs"
  },
  "license": "MIT",
  "dependencies": {
    "@deeptracer/core": "0.4.0",
    "@deeptracer/node": "0.4.0",
    "@deeptracer/react": "0.4.0"
  },
  "peerDependencies": {
    "next": ">=14",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsup",
    "dev": "tsup --watch"
  }
}
